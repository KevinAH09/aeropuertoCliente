<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<fx:root id="AnchorPane" prefHeight="400.0" prefWidth="600.0" type="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.una.aeropuertocliente.controllers.MantenimientoAerolineasController">
   <children>
      <StackPane prefHeight="493.0" prefWidth="701.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <children>
                        <Label text="Mantenimiento de Aerolineas">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="60.0" prefWidth="687.0" spacing="20.0">
                     <children>
                        <Label text="ID: " />
                        <JFXTextField fx:id="txtId" prefHeight="25.0" prefWidth="251.0" />
                        <Label text="Nombre:" />
                        <JFXTextField fx:id="txtNombre" prefHeight="25.0" prefWidth="244.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="60.0" prefWidth="200.0" spacing="20.0">
                     <children>
                        <Label text="Responsable:" />
                        <JFXTextField fx:id="txtResponsable" prefHeight="25.0" prefWidth="251.0" />
                        <Label text="Estado:" />
                        <JFXComboBox fx:id="cmbEstado" prefHeight="25.0" prefWidth="230.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="701.0" spacing="10.0">
                     <children>
                        <JFXButton fx:id="txtGuardar" onAction="#onActionGuardar" text="Guardar" />
                        <JFXButton fx:id="btnCancelar" onAction="#onActionCancelar" text="Cancelar" />
                        <JFXButton fx:id="btnEditar" onAction="#onActionEditar" text="Editar" />
                     </children>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </HBox>
                  <HBox prefHeight="200.0" prefWidth="200.0" spacing="20.0">
                     <children>
                        <JFXTreeTableView fx:id="tableAviones" prefHeight="128.0" prefWidth="689.0" />
                     </children>
                     <padding>
                        <Insets left="10.0" right="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</fx:root>
